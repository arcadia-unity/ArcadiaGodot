{;; Default configuration
 ;; merged with project/configuration.edn

 ;; repl ports, comment out or set to nil to disable
 :udp-repl 11211
 :socket-repl 5571
 :nrepl 3722

 ;; TODO
 :nrepl-auto-completion false

 ;; Source paths for Clojure files to include
 :source-paths ["src"]

 ;; Declare the Clojure namespaces you intend to include in your exported game
 :export-namespaces []
 ;; folder with exported dlls
 :export-folder "dlls"

 :assembly-paths []

 ;; enables filewatcher to reload .clj files when changed
 :reload-on-change true}
